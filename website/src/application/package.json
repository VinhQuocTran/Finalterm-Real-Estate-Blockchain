{
    "name": "asset-transfer-basic",
    "version": "1.0.0",
    "description": "Asset-transfer-basic application implemented in JavaScript",
    "engines": {
        "node": ">=14.14",
        "npm": ">=6"
    },
    "scripts": {
        "lint": "eslint *.js",
        "pretest": "npm run lint",
        "migration:create": "npx sequelize-cli migration:create",
        "migrate": "npx sequelize-cli db:migrate",
        "migrate:rollback": "npx sequelize-cli db:migrate:undo",
        "start": "node server.js",
        "dev": "nodemon server.js",
        "swagger-autogen": "node ./docs/swagger.js",
        "start:prod": "NODE_ENV=production nodemon server.js"
    },
    "engineStrict": true,
    "author": "Hyperledger",
    "license": "Apache-2.0",
    "dependencies": {
        "bcrypt": "^5.1.1",
        "cloudinary": "^1.41.0",
        "cookie-parser": "^1.4.6",
        "cors": "^2.8.5",
        "dotenv": "^16.3.1",
        "ejs": "^3.1.9",
        "express": "^4.18.2",
        "fabric-ca-client": "^2.2.19",
        "fabric-network": "^2.2.19",
        "html-to-text": "^9.0.5",
        "jsonwebtoken": "^9.0.2",
        "multer": "^1.4.5-lts.1",
        "mysql2": "^3.6.5",
        "nodemailer": "^6.9.7",
        "nodemon": "^3.0.2",
        "sequelize": "^6.35.1",
        "sequelize-cli": "^6.6.2",
        "swagger-autogen": "^2.23.7",
        "swagger-jsdoc": "^6.2.8",
        "swagger-ui-express": "^5.0.0"
    },
    "devDependencies": {
        "eslint": "^7.32.0"
    }
}
